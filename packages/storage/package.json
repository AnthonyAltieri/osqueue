{
  "name": "@osqueue/storage",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./memory": {
      "types": "./dist/memory.d.ts",
      "import": "./dist/memory.js"
    },
    "./s3": {
      "types": "./dist/s3.d.ts",
      "import": "./dist/s3.js"
    },
    "./gcs": {
      "types": "./dist/gcs.d.ts",
      "import": "./dist/gcs.js"
    },
    "./throttled": {
      "types": "./dist/throttled.d.ts",
      "import": "./dist/throttled.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts src/memory.ts src/s3.ts src/gcs.ts src/throttled.ts --format esm --dts --target node20 --clean",
    "test": "vitest run test/**/*.test.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@osqueue/types": "workspace:*"
  },
  "optionalDependencies": {
    "@google-cloud/storage": "^7.14.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  }
}
